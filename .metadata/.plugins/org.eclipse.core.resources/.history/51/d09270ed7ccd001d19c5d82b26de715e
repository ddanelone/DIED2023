public class Temperatura {
	private String ciudad;
	private Float Temperatura[];

	private Short indice=0;
	
	public Float Registro(String ciudad, Short max) {
		this.ciudad = "-";
		this.Temperatura = new Array[max];
		
	};
		
	public  Float Registro(String ciudad, Short max) {
		this.ciudad = ciudad;
		this.Temperatura[] = new Array[max];
		
	};

}
